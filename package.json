{
  "name": "light-ui",
  "version": "0.0.1",
  "description": "Components of hacknical",
  "main": "./lib/index.js",
  "scripts": {
    "babel": "babel ./components --out-dir ./lib",
    "css": "cpx \"./components/**/*.css\" ./lib",
    "clean": "rm -rf ./lib",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "build": "cross-env NODE_ENV=production npm run babel && npm run css",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "gh-pages:clean": "rimraf _gh-pages",
    "gh-pages:build": "$(npm bin)/build-storybook -o _gh-pages",
    "gh-pages:copy-public": "cp public/* _gh-pages/$1",
    "gh-pages:publish": "$(npm bin)/git-directory-deploy --directory _gh-pages",
    "gh-pages": "npm run gh-pages:build && npm run gh-pages:copy-public && npm run gh-pages:publish",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "author": "ecmadao",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.15.2",
    "open-color": "^1.4.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-portal": "^3.0.0",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.24.1",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-deployer": "^1.2.0",
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.14.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "git-directory-deploy": "^1.5.1",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3"
  }
}
