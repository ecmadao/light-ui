{
  "name": "light-ui",
  "version": "0.0.32",
  "description": "Components of hacknical",
  "main": "./lib/index.js",
  "scripts": {
    "babel": "babel ./components --out-dir ./lib",
    "css": "cpx \"./components/**/*.css\" ./lib",
    "clean": "rm -rf ./lib",
    "prebuild": "npm run clean",
    "prepublish": "in-publish && safe-publish-latest && npm run build || not-in-publish",
    "tag": "git tag v$npm_package_version",
    "postversion": "git commit package.json -m \"Version $npm_package_version\" && npm run tag && git push && git push --tags && npm publish --registry=https://registry.npmjs.org/",
    "build": "cross-env NODE_ENV=production npm run babel && npm run css",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "storybook-deploy": "npm run storybook-pro && npm run deploy-storybook",
    "storybook-pro": "build-storybook -c .storybook -o .out",
    "eslint": "./node_modules/.bin/eslint components"
  },
  "author": "ecmadao",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.15.2",
    "object-assign": "^4.1.1",
    "open-color": "^1.4.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-portal": "^3.0.0",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.24.1",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-deployer": "^1.2.0",
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.14.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "file-loader": "^0.9.0",
    "in-publish": "^2.0.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "safe-publish-latest": "^1.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3"
  }
}
