{
  "name": "hacknical-components",
  "version": "0.0.1",
  "description": "Components of hacknical",
  "main": "./lib/index.js",
  "scripts": {
    "babel": "babel ./components --out-dir ./lib",
    "css": "cpx \"./components/**/*.css\" ./lib",
    "clean": "rm -rf ./lib",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "build": "cross-env NODE_ENV=production npm run babel && npm run css",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "author": "ecmadao",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "jquery": "^3.1.1",
    "moment": "^2.15.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-portal": "^3.0.0",
    "open-color": "^1.4.0",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.24.1",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-deployer": "^1.2.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.14.0",
    "babel-polyfill": "^6.16.0",
    "autoprefixer": "^6.5.3",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "cross-env": "^3.1.3",
    "cpx": "^1.5.0"
  }
}
